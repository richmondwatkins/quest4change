extends ../shared/template

block content

    h4 Group Lock
    input(type='checkbox', value='1', checked='checked')


    h3= group.name

    h3 Member's in this Group
    each member in group.members
      h5= member.userName

    if(user._id.toString() === group.owner)
      h3 This is the Group Creator
      h3= user.userName




block scripts
  script(src='/js/vendor/jquery.switchButton.js')
